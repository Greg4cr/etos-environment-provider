{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "iut": {
            "type": "object",
            "properties": {
                "id": { "type": "string" },
                "checkin": {},
                "checkout": {},
                "list": {
                    "properties": {
                        "possible": {},
                        "available": {}
                    }
                },
                "prepare": {
                    "type": "object",
                    "properties": {
                        "commands": {
                            "items": {
                                "properties": {
                                    "command": {
                                        "items": {
                                            "type": "string"
                                        },
                                        "type": "array"
                                    },
                                    "threaded": { "type": "boolean" }
                                },
                                "required": [
                                    "command"
                                ],
                                "type": "object"
                            },
                            "type": "array"
                        },
                        "environment": {
                            "type": "object"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "commands"
                    ]
                }
            },
            "required": ["id", "list"],
            "additionalProperties": false
        }
    },
    "required": ["iut"],
    "additionalProperties": false
}
